# Sample environment configuration for AI Negotiation Demo App
# Copy this file to .env and fill in with your actual values before running the app.

# Azure OpenAI configuration
REACT_APP_OPENAI_ENDPOINT=https://<YOUR_OPENAI_RESOURCE>.openai.azure.com
REACT_APP_OPENAI_API_KEY=<YOUR_OPENAI_API_KEY>
REACT_APP_OPENAI_DEPLOYMENT=<YOUR_OPENAI_DEPLOYMENT_NAME>

# Azure Cognitive Search configuration
REACT_APP_AZURE_SEARCH_ENDPOINT=https://<YOUR_SEARCH_SERVICE>.search.windows.net
REACT_APP_AZURE_SEARCH_API_KEY=<YOUR_SEARCH_API_KEY>
REACT_APP_SEARCH_INDEX_NAME=<YOUR_SEARCH_INDEX_NAME>

# Toggle alternative RAG service implementation
REACT_APP_USE_ALT_RAG=true

# Alt RAG endpoint for combined search+chat service
REACT_APP_ALT_RAG_ENDPOINT=https://<YOUR_ALT_RAG_ENDPOINT_URL>
