{
  "name": "angular-standalone-sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run postinstall:msal-angular && ng build",
    "postinstall:msal-angular": "node ../sample-scripts/useMsalAngularTarball.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "test:e2e": "jest",
    "build:ssr": "ng build && ng run angular-standalone-sample:server:production",
    "dev:ssr": "ng run angular-standalone-sample:serve-ssr",
    "serve:ssr": "node dist/angular-standalone-sample/server/main.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~19.1.0",
    "@angular/cdk": "^19.2.8",
    "@angular/common": "~19.1.0",
    "@angular/compiler": "~19.1.0",
    "@angular/core": "~19.1.0",
    "@angular/forms": "~19.1.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "~19.1.0",
    "@angular/platform-browser-dynamic": "~19.1.0",
    "@angular/platform-server": "^19.2.7",
    "@angular/router": "~19.1.0",
    "@azure/msal-angular": "^4.0.0",
    "@azure/msal-browser": "^4.0.0",
    "@azure/openai": "^2.0.0",
    "@fortawesome/angular-fontawesome": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@microsoft/applicationinsights-web": "^3.3.6",
    "@nguniversal/builders": "^16.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "angular-split": "^19.0.0",
    "bootstrap": "^5.3.5",
    "jszip": "^3.10.1",
    "mammoth": "^1.9.0",
    "marked": "^15.0.7",
    "ngx-doc-viewer": "^15.0.1",
    "ngx-markdown": "^19.1.1",
    "openai": "^4.62.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.8",
    "@angular/cli": "~19.1.0",
    "@angular/compiler-cli": "~19.1.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.12",
    "@types/jszip": "^3.4.0",
    "jasmine-core": "~5.1.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.7.3"
  }
}
